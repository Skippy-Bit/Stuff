{
    "version": "2.0.0",
    "tasks": [{
            "label": "Flutter - Build Runner (Watch)",
            "type": "shell",
            "command": "flutter packages pub run build_runner watch --delete-conflicting-outputs"
        },
        {
            "label": "Flutter - Build Runner (Build)",
            "type": "shell",
            "command": "flutter packages pub run build_runner build --delete-conflicting-outputs"
        },
        {
            "label": "Flutter - Clean",
            "type": "shell",
            "command": "flutter clean"
        },
        {
            "label": "Flutter - Clean run",
            "type": "shell",
            "command": "flutter clean | flutter run",
        },
        {
            "label": "Generate Launcher Icons",
            "type": "shell",
            "command": "flutter packages pub run flutter_launcher_icons:main"
        }
    ],
}